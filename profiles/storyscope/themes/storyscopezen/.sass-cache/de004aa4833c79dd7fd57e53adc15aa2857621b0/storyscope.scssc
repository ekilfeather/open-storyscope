/* =============================================================================
   Links
   ========================================================================== */

/*
 * The order of link states are based on Eric Meyer's article:
 * http://meyerweb.com/eric/thoughts/2007/06/11/who-ordered-the-link-states
 */

@import "compass/typography/links";

a:link {
  color: $link-gray;
  text-decoration: none;
  border-bottom: solid 1px transparent;
  &.has-image {
    border-bottom: none;
  }
}

a:visited {
  color: $link-visited;
  border-bottom: solid 1px $light-gray;
}

a:hover,
a:focus,
a:active {
  color: $hover-gray;
  border-bottom: solid 1px $light-gray;
}


/* Headings */
h1,
h2,
h3,
h4,
h5,
h6{
  line-height: 1.3em;
  font-weight: normal;
  a:link,
  a:visited {
    color: inherit;
  }
}
h2.title {
  font-size: 1.2em;
}
h3 {
  color: $med-gray;
}
p {
  margin: 0 0 1.5em 0;
  &:first-child {
    margin-top: 1em;
  }
}
/* Navigation */
#navigation {
  a {
    @include link-colors($dark-gray, $med-gray);
    border-bottom: none;
    font-weight: bold;
  }
}
/* Admin Section */

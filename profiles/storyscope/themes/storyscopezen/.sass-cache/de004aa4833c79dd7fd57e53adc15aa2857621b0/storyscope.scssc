  h2.pane-title {
    color: $med-gray;
  }
  .pane-field-collection-form-buttons {
    .form-submit {
      background-color: $very-light-gray;
      padding: 0.25em 2em 0.25em 0.5em;
      font-weight: bold;
      color: $dark-gray;
    }
  }
}

.facetapi-limit-link{
  margin-left: 30px;
}

/* Basic Styling for Editing Narratives */
#edit-field-narrative-set-section {
  .entityreference-view-widget > .widget-left {
      width: 33%;
  }
  .entityreference-view-widget > .widget-right {
      width: 66%;
  }
  .field-multiple-table td.field-multiple-drag {
    width: 13px;
  }
  a.tabledrag-handle .handle {
    width: 10px;
  }
  .cke_contents {
    height: 100px !important;
  }
}

/* Hide all text format help */
.text-format-wrapper .filter-wrapper {
  display: none;
}


/**
 * form Styling
 **/

.form-wrapper {
  margin-bottom: 1em;
}
.fieldset-wrapper {
  .form-wrapper {
    border-bottom: solid 1px #eee;
    margin-bottom: 1em;
    .fieldset-description {
      color: $med-gray;
      font-size: 0.95em !important;
    }
  }
}
.form-item {
//margin: 0.5em 0 !important;
  .description {
    color: $med-gray;
    font-size: 0.95em !important;
  }
}

.text-format-wrapper {
  .description {
    color: $med-gray;
    font-size: 0.95em !important;
  }
}

// Custom Pagers
.story-section-pager,
.narrative-set-pager {
  @include inline-list;
  padding-left: 0;
    li {
      a {
        background: $light-gray;
        padding: 0.25em 0.5em;
        margin-right: 5px;
        @include link-colors($dark-gray, $very-light-gray);
        &.active,
        &:hover {
          background: $gray-green;
        }
        &.active{
          @include link-colors($very-light-gray);
        }
      }
    }
}

/* Add existing events page styling tweaks */
.view-dossier-events-add-existing-search-api,
.view-object-story-objects-add-existing-search-api,
.view-object-story-events-add-existing-search-api {
  .view-header .button{
    padding-bottom: 15px;
  }
}

#views-exposed-form-object-story-events-add-existing-search-api-page-add-event-dossier-context-internal,
#views-exposed-form-dossier-events-add-existing-search-api-page-add-event-search-internal,
#views-exposed-form-dossier-object-stories-add-existing-search-api-page-add-object-story-internal,
#views-exposed-form-object-story-objects-add-existing-search-api-page-add-object-dossier-context-internal,
#views-exposed-form-dossier-events-search-api-dossier-events-page {
  margin-left: 25px;
  margin-top: 12px;
  margin-bottom: 14px;
}

.region-sidebar-second h1 {
  border-bottom: 2px solid #ccc;
  padding-bottom: 10px;
  margin-top: 25px;
  margin-bottom: 0px;
}
.block-facetapi {
  padding-bottom: 10px;
}

/* facet list styling */
.item-list .facetapi-facetapi-links {
  margin: 10px 0 5px 0;
  .leaf a {
    color: $dark-gray;
  }
  a.facetapi-limit-link {
    color: $link-gray;
  }
}
.underline {
  width: 100%;
  border-bottom: solid 2px $light-gray;
  position: absolute;
  bottom: 0;
}
 .region-sidebar-second {
  h2.block-title{ /* Block title */
      text-transform: uppercase;
      font-size: 0.92308em;
      line-height: 1.5em;
      padding-left: 30px;
      padding: 5px 5px 5px 15px;
      border-bottom: solid 1px #eee;
      border-top: solid 1px #c4c4c4;
      margin-bottom: 8px;
  }
  h2 {
    margin-top: 25px;
    margin-bottom: 10px;
    padding-bottom: 10px;
    border-bottom: solid 2px $light-gray;
  }
}

div.tabs h1.title {
  width: 80% !important;
}

/* Similarity search form */
#query-form {

}

#similarity-search-type {
  visibility: hidden;
  display: none;
}

#edit-context {
    width: 300px;
}

#edit-query-text {
    width: 100%;
}

#similarity-query-field-wrap {

}

#similarity-query-field {
    width: 89%;
}

#similarity-query-search {
    float: right;
    width: 9%;
}

#similarity-query-search input {
    width: 100%;
}

#similarity-query-search div {
   float: right;
   margin: 0;
}

.build-query-fields div {
    margin: 0;
}

.similarity-form-small-divider {
    width: 50px;
}

#similarity-facets {
    float: right;
    width: 23%;
    margin-left: 2%;
    margin-top: 45px;
}

#similarity-context {
    float: right;
    margin: 5px 10px 0 0;
}

.similarity-total-estimate {
    padding-top: 8px;
}

.similarity-wrap {
    margin-top: 30px;
    float: left;
    width: 720px;
    border-top: 1px solid #E2903D;
    padding-top: 0.5em;
}

.similarity-checkbox {
    float: left;
    margin: 0;
}

.similarity-checkbox .form-item {
    margin: 0 0 5px 0;
}

.similarity-block {
    clear: both;
}

.similarity-block h4 {
    font-weight: bold;
    margin: 0.2em 0 0.2em 0;
    font-size: 1.2em;
}

.similarity-image-wrap {
    float: left;
    margin: 0 10px 0 0;
    width: 25%;
}

.similarity-image {
    max-height: 150px;
    overflow: hidden;
    display: block;
}

.similarity-text-wrap {
    overflow: hidden;
}

.similarity-buttons {
    clear: both;
    float: right;
}    

.similarity-item-menu {
	float:left;
}

.similarity-item-menu input {
	float:left;
}

.similarity-item-ignore-menu, .similarity-item-add-menu {
	margin-left: 20px !important;
	float:left;
}

.similarity-item-add-menu {
    font-weight: bold;
}

.similarity-item-ignore-menu .ignore-title {
    text-decoration: underline;
}

.similarity-item-ignore-menu ul {
  display: none;
  visibility: hidden;
  margin: 0;
  padding: 0;
}

.similarity-item-ignore-menu ul li {
  list-style-type: none;
}

.similarity-item-ignore-menu:hover ul {
  display: block;
  visibility: visible;
  position: absolute;
  background-color: #fff;
  padding: 5px;
  border: 1px #eee solid;
  border-top: 0 none;
}

.similarity-paging-wrap {
    clear: both;
    text-align: center;
    padding-top: 40px;
}

.similarity-paging {
    margin: 0 auto;
}

.similarity-bottom-buttons {
  margin: 4em 0 6em 0;  
}

.clear {
	clear: both;
}

/* Similarity with views */
.similarity-results {
    width: 75%;
}

.similarity-wrap:nth-child(2n) {
    float: left;
    margin-left: 51%;
    margin-right: -100%;
    padding-right: 0;
    width: 49%;
}

.similarity-wrap:nth-child(2n+1) {
    clear: left;
    float: left;
    margin-left: 0;
    margin-right: -50%;
    padding-left: 0;
    width: 49%;
}

.similarity-wrap {
    -moz-box-sizing: border-box;
    word-wrap: break-word;
}

/* Similarity search item detail */
.similarity-search-item-header {

}

.similarity-search-item-image {
    width: 49%;
}

.similarity-search-item-image img {
    max-height: 300px;
}

.similarity-search-item-title {
  margin-top: 7em;
  overflow: hidden;
}

.similarity-search-item-content {

}

.similarity-search-item-info {
    float: left;
    margin-left: 0;
    margin-right: -50%;
    width: 48%;
}

.similarity-search-item-buttons {
    margin-bottom: 1.5em;
    padding-top: 0.5em;
}

.similarity-search-item-info-label {
    color: #DDDDDD;
}

.similarity-search-item-description {
    float: left;
    margin-top: 1em;
    margin-left: 50%;
    margin-right: -100%;
    padding-right: 0;
    width: 50%;
}

.similarity-search-item-description-no-info {
    margin-top: 1em;
    padding-right: 0;
}

.similarity-search-item-description-no-info .similarity-search-item-description-text {
    border: 0px none;
    padding-right: 1em;
}

.similarity-search-item-description-no-info .similarity-search-item-buttons {
    padding-top: 1.5em;
}

.similarity-search-item-description-no-info .button a {
    margin-left: 1em;
}

/* Similarity search child item detail */
.similarity-search-item-childs {

}

.similarity-search-item-child {
    clear: both;
    border-top: 1px #ddd solid;
    padding-top: 0.2em;
    margin-bottom: 1em;
}

.similarity-search-item-child-image {
    width: 28%;
    float: left;
}

.similarity-search-item-clear {
    clear: both;
}

.similarity-search-item-type {
    float:right;
}

.similarity-search-item-dossier {
    color: #F17BA9;
    border-color: #F17BA9;
}

.similarity-search-item-object-story {
    color: #F9CC67;
    border-color: #F9CC67;
}

.similarity-search-item-object {
    color: #E2903D;
    border-color: #E2903D;
}

.similarity-search-item-event {
    color: #7AC953;
    border-color: #7AC953;
}

.similarity-search-item-event-reference {
    color: #00A99D;
    border-color: #00A99D;
}

.similarity-search-item-child-image img {
    max-height: 150px;
}

.similarity-search-item-child-content {
    margin-left: 30%;
    width: 70%;
}

.similarity-search-item-child-content h4 {
    margin: 0;
    font-weight: bold;
}

.similarity-search-item-child-title {

}

.similarity-search-item-child-description {
    color: #222;
}

/* Similarity search autocomplete - used in events form */
.similarity-search-autocomplete-row {
    max-height: 50px;
}

.similarity-search-autocomplete-left {
    display: table-cell;
    vertical-align: middle;
}

.similarity-search-autocomplete-right {
    display: block;
    float: right;
    color: #4e4e4e;
    margin-right: 0.2em;
    max-height: 50px;
    overflow: hidden;
}

.similarity-search-autocomplete-right img {
    max-height: 50px;
    max-width: 150px;
}

/* Special styling for the Start New Story link on the Dossier page to make it look like a button*/
.pane-storyscope-content-dossier-start-new-story .pane-content ul li:first-child {
  background-color: #a6cef3;
  border: solid 1px #8cbae4;
  padding: 0.5em 1em;
  margin: 0.25em 0.25em 0.5em;
  a {
    color: $dark-gray;
  }
}


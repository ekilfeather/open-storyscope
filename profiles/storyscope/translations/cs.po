msgstr "Ve výchozím nastavení neodhlásit"

#: profiles/storyscope/modules/contrib/google_analytics/googleanalytics.admin.js
msgid "Outbound links"
msgstr "Odchozí odkazy"

#: profiles/storyscope/modules/contrib/google_analytics/googleanalytics.admin.js
msgid "Mailto links"
msgstr "Mailto odkazy"

#: profiles/storyscope/modules/contrib/google_analytics/googleanalytics.admin.js
msgid "Downloads"
msgstr "Stahování"

#: profiles/storyscope/modules/contrib/google_analytics/googleanalytics.admin.js
msgid "Not tracked"
msgstr "Nesledován"

#: profiles/storyscope/modules/contrib/google_analytics/googleanalytics.admin.js
msgid "@items enabled"
msgstr "@items povoleny"

#: profiles/storyscope/modules/contrib/google_analytics/googleanalytics.admin.js
msgid "Site search"
msgstr "Vyhledávání na webu"

#: profiles/storyscope/modules/contrib/google_analytics/googleanalytics.admin.js
msgid "AdSense ads"
msgstr "AdSense reklamy"

#: profiles/storyscope/modules/contrib/google_analytics/googleanalytics.admin.js
msgid "DoubleClick data"
msgstr "DoubleClick data"

#: profiles/storyscope/modules/contrib/google_analytics/googleanalytics.admin.js
msgid "A single domain"
msgstr "Jedna doména"

#: profiles/storyscope/modules/contrib/google_analytics/googleanalytics.admin.js
